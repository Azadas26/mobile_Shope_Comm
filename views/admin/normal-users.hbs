<div class="container-fluid">
    <div class="row">
        <div class="col-md-12 mt-5 p-5">
            <table class="table mt-5 text-light" id="usersusers">
                <h1 class="text-light text-center"><b><i>Available Users</i></b></h1>
                <thead>
                    <tr>
                        <th scope="col">No</th>
                        <th scope="col">user Name</th>
                        <th scope="col">Email Address</th>
                        <th scope="col">Password</th>
                        <th scope="col">Gender</th>
                        <th scope="col">Age</th>
                       
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr>
                        <td>{{@index}}</td>
                        <td>{{this.name}}</td>
                        <td>{{this.email}}</td>
                        <td>{{this.password}}</td>
                        <td>{{this.gender}}</td>
                        <td>{{this.age}}</td>
                        
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>